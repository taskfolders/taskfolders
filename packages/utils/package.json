{
  "name": "@taskfolders/utils",
  "version": "0.0.1",
  "description": "Generic codign utilities and helpers",
  "access": "public",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taskfolders/taskfolders.git"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "publish": "npm publish --access public",
    "test:watch": "vitest watch --hideSkippedTests --changed",
    "test:ci": "sh ./bin/test--ci.sh",
    "test": "vitest --run"
  },
  "exports": {
    "./logger/node": "./dist/esm/logger/node/index.js",
    "./logger/LoggerMock": "./dist/esm/logger/node/LoggerMock.js",
    "./logger/browser": "./dist/esm/logger/browser/index.js",
    "./logger/node.start": "./dist/esm/logger/node.start.js",
    "./dependencies": "./dist/esm/dependencies/index.js",
    "./markdown": "./dist/esm/markdown/index.js",
    "./shell": "./dist/esm/shell/index.js",
    "./dependencies/DCTester": "./dist/esm/dependencies/DCTester.js",
    "./native/array/*": "./dist/esm/native/array/*.js",
    "./fs/*": "./dist/esm/fs/*.js",
    "./vendors/npm": "./dist/esm/vendors/npm/index.js",
    "./vendors/*": "./dist/esm/vendors/*.js",
    "./types/*": "./dist/esm/types/*.js",
    "./*": "./dist/esm/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/*",
        "dist/esm/*/index.d.ts"
      ]
    }
  },
  "keywords": [],
  "author": {
    "name": "Francisco Garcia",
    "url": "https://taskfolders.com"
  },
  "license": "MIT",
  "dependencies": {
    "source-map-support": "^0.5.21",
    "yaml": "^2.4.0"
  },
  "devDependencies": {
    "memfs": "^4.7.7"
  }
}
