{
  "name": "@taskfolders/utils",
  "version": "0.0.1",
  "description": "Generic codign utilities and helpers",
  "access": "public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taskfolders/open.git"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "publish": "npm publish --access public",
    "test:ci": "npx vitest --run --exclude '**/*_noci*' --allowOnly=false",
    "test": "npx vitest --run"
  },
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/*",
        "dist/types/*/index.d.ts"
      ]
    }
  },
  "keywords": [],
  "author": {
    "name": "Francisco Garcia",
    "url": "https://taskfolders.com"
  },
  "license": "MIT",
  "dependencies": {
    "source-map-support": "^0.5.21"
  }
}
